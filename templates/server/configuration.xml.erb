<?xml version="1.0" encoding="UTF-8"?>
<config>
  <roles>
    <role0>process-server-<%= @server_category %></role0>
  </roles>
  <license>
    <client>
      <keyserver>
        <host><%= @key_server %></host>
      </keyserver>
    </client>
    <keyserver>
      <enabled>false</enabled>
    </keyserver>
  </license>
  <server>
    <documentflow>
      <security>
        <roles>
          <role0>
            <canpreview>true</canpreview>
            <name>anonymousrole</name>
            <canadmin>false</canadmin>
         </role0>
         <role1>
           <name>adminrole</name>
           <canadmin>true</canadmin>
           <canpreview>true</canpreview>
         </role1>
       </roles>
     </security>
    </documentflow>
    <ondemand>
      <security>
        <enabled>false</enabled>
        <roles>
          <role0>
            <name>anonymousrole</name>
            <schema/>
          </role0>
        </roles>
      </security>
    </ondemand>
  </server>
  <templateprocessor>
    <cache>
      <max-memory-entries>100</max-memory-entries>
      <max-disk-entries>200</max-disk-entries>
      <max-disk-space>500</max-disk-space>
    </cache>
    <fonts>
      <directories>
          <directory0><%= @fonts_dir %></directory0>
      </directories>
    </fonts>
  </templateprocessor>
</config>
